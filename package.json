{
  "name": "ts-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn workspace @ts-monorepo/common build && yarn install"
  },
  "workspaces": [
    "packages/*"
  ],
  "installConfig": {
    "hoistingLimits": "none",
    "selfReferences": false
  },
  "packageManager": "yarn@4.1.1"
}
