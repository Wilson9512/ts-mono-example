{
  "name": "ts-monorepo",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn workspaces foreach -A run build"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "typescript": "^5.5.3"
  }
}
