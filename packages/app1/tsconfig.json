// prettier-ignore
{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "moduleSuffixes": [".android", ".ios",".native", ""],
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["src/lib/*"],
      "@navigation": ["src/navigation"],
      "@navigation/*": ["src/navigation/*"],
      "@screens": ["src/screens"],
      "@screens/*": ["src/screens/*"],
    },
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react-native"
  }
}
